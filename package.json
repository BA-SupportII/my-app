{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:3001": "next dev --turbopack -p 3001",
    "build": "next build",
    "start": "next start -p 3000",
    "start:3001": "next start -p 3001",
    "kill:3000": "kill-port 3000",
    "kill:3001": "kill-port 3001",
    "dev:clean": "npm run kill:3000 && next dev --turbopack",
    "start:clean": "npm run kill:3000 && next start -p 3000",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "telemetry:off": "next telemetry disable"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "kill-port": "^2.0.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
